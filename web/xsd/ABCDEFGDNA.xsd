	<!--<xs:import namespace="http://www.synthesys.info/ABCDEFG/1.0" schemaLocation="http://www.geocase.eu/sites/geocase.biodiv.naturkundemuseum-berlin.de/documentation/abcdefg.xsd"/>-->
