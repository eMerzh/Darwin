				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="TelephoneNumber" type="TelephoneNumber" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>Telephone or fax number</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
